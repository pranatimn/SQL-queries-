


DROP TABLE IF EXISTS OLYMPICS_HISTORY;
CREATE TABLE IF NOT EXISTS OLYMPICS_HISTORY
(
ID INT,
NAME VARCHAR,
SEX VARCHAR,
AGE VARCHAR,
HEIGHT VARCHAR,
WEIGHT VARCHAR,
TEAM VARCHAR,
NOC VARCHAR,
GAMES VARCHAR, 
YEAR INT,
SEASON VARCHAR,
CITY VARCHAR,
SPORT VARCHAR,
EVENT VARCHAR,
MODEL VARCHAR
);

DROP TABLE IF EXISTS OLYMPICS_HISTORY_NOC_REGION;
CREATE TABLE IF NOT EXISTS OLYMPICS_HISTORY_NOC_REGION
(
NOC VARCHAR,
REGION VARCHAR,
NOTES VARCHAR,
);




